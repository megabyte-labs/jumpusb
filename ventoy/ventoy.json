{
  "auto_install": [
    {
      "autosel": 1,
      "image": "/iso/qubes-custom.iso",
      "template": ["/ventoy/kickstart/qubes-desktop.cfg"],
      "timeout": 30
    },
    {
      "parent": "/iso/fedora",
      "template": [
        "/ventoy/scripts/fedora/kickstart-server.cfg",
        "/ventoy/scripts/fedora/kickstart-workstation-minimal.cfg",
        "/ventoy/scripts/fedora/kickstart-workstation-full.cfg"
      ]
    },
    {
      "parent": "/iso/qubes",
      "template": [
        "/ventoy/scripts/qubes/kickstart-desktop-minimal.cfg",
        "/ventoy/scripts/qubes/kickstart-desktop-full.cfg"
      ]
    },
    {
      "parent": "/iso/ubuntu",
      "template": [
        "/ventoy/scripts/ubuntu/cloud-init-server",
        "/ventoy/scripts/ubuntu/cloud-init-desktop-minimal",
        "/ventoy/scripts/ubuntu/cloud-init-desktop-full"
      ]
    },
    {
      "parent": "/iso/windows",
      "template": ["/ventoy/scripts/windows-11-minimal.xml", "/ventoy/scripts/windows-11-full.xml"]
    }
  ],
  "control": [
    { "VTOY_DEFAULT_SEARCH_ROOT": "/iso" },
    { "VTOY_MENU_TIMEOUT": "30" },
    { "VTOY_DEFAULT_IMAGE": "/iso/qubes-custom.iso" }
  ],
  "injection": [
    {
      "archive": "/assets/playbooks.tar.gz",
      "parent": "/iso/fedora"
    },
    {
      "archive": "/assets/playbooks.tar.gz",
      "parent": "/iso/qubes"
    },
    {
      "archive": "/assets/playbooks.tar.gz",
      "parent": "/iso/ubuntu"
    },
    {
      "archive": "/assets/playbooks.zip",
      "parent": "/iso/windows"
    }
  ],
  "menu_alias": [
    {
      "alias": "Install Fedora 36",
      "image": "/iso/fedora/fedora-server-36.iso"
    },
    {
      "alias": "Install Qubes 4.1",
      "image": "/iso/qubes/qubes-4.1.0.iso"
    },
    {
      "alias": "Install Ubuntu 22.04 (Jammy Jellyfish)",
      "image": "/iso/ubuntu/ubuntu-server-22.04.iso"
    },
    {
      "alias": "Install Windows 11",
      "dir": "/iso/windows/windows-11.iso"
    },
    {
      "alias": "Run ArchLinux (Live)",
      "image": "/iso/live/archlinux.iso"
    },
    {
      "alias": "Run CloneZilla (Live)",
      "image": "/iso/live/clonezilla.iso"
    },
    {
      "alias": "Run ElementaryOS (Live)",
      "image": "/iso/live/elementary.iso"
    },
    {
      "alias": "Run Kali (Live)",
      "image": "/iso/live/kali.iso"
    },
    {
      "alias": "Run Kaspersky Rescue Disk (Live)",
      "image": "/iso/live/kaspersky-rescue-disk.iso"
    },
    {
      "alias": "Run Ubuntu 22.04 (Jammy Jellyfish) (Live)",
      "image": "/iso/live/ubuntu.iso"
    },
    {
      "alias": "Run ZorinOS (Live)",
      "image": "/iso/live/zorin.iso"
    }
  ],
  "menu_class": [
    {
      "class": "arch",
      "key": "archlinux"
    },
    {
      "class": "clonezilla",
      "key": "clonezilla"
    },
    {
      "class": "elementary",
      "key": "elementary"
    },
    {
      "class": "kali",
      "key": "kali"
    },
    {
      "class": "krd",
      "key": "kaspersky"
    },
    {
      "class": "ubuntu",
      "key": "ubuntu"
    },
    {
      "class": "zorin",
      "key": "zorin"
    },
    {
      "class": "arch",
      "parent": "/iso/archlinux"
    },
    {
      "class": "fedora",
      "parent": "/iso/fedora"
    },
    {
      "class": "qubes",
      "parent": "/iso/qubes"
    },
    {
      "class": "ubuntu",
      "parent": "/iso/ubuntu"
    },
    {
      "class": "windows",
      "parent": "/iso/windows"
    }
  ],
  "persistence": [
    {
      "backend": ["/persistence/archlinux-live.dat", "/persistence/archlinux-live-alternate.dat"],
      "image": "/iso/live/archlinux.iso"
    },
    {
      "backend": ["/persistence/elementary-live.dat", "/persistence/elementary-live-alternate.dat"],
      "image": "/iso/live/elementary.iso"
    },
    {
      "backend": ["/persistence/kali-live.dat", "/persistence/kali-live-alternate.dat"],
      "image": "/iso/live/kali.iso"
    },
    {
      "backend": ["/persistence/krd.dat", "/persistence/krd-alternate.dat"],
      "image": "/iso/live/kaspersky-rescue-disk.iso"
    },
    {
      "backend": ["/persistence/ubuntu-live.dat", "/persistence/ubuntu-live-alternate.dat"],
      "image": "/iso/live/ubuntu.iso"
    },
    {
      "backend": ["/persistence/zorin-live.dat", "/persistence/zorin-live-alternate.dat"],
      "image": "/iso/live/zorin.iso"
    }
  ],
  "theme": {
    "default_file": 1,
    "display_mode": "GUI",
    "file": ["/ventoy/themes/default/theme.txt"],
    "fonts": [
      "/ventoy/fonts/dejavu_sans_12.pf2",
      "/ventoy/fonts/dejavu_sans_14.pf2",
      "/ventoy/fonts/dejavu_sans_16.pf2",
      "/ventoy/fonts/dejavu_sans_24.pf2",
      "/ventoy/fonts/dejavu_sans_32.pf2",
      "/ventoy/fonts/dejavu_sans_48.pf2",
      "/ventoy/fonts/terminus-12.pf2:",
      "/ventoy/fonts/terminus-14.pf2",
      "/ventoy/fonts/terminus-16.pf2",
      "/ventoy/fonts/terminus-18.pf2"
    ],
    "gfxmode": "3440x1440,auto",
    "serial_param": "--unit=0 --speed=9600",
    "ventoy_color": "#efefef",
    "ventoy_left": "5%",
    "ventoy_top": "95%"
  }
}
